#include<stdio.h>
int main(){
    char a[2];
    printf("Enter the operator\n");
    scanf("%s",a);
    if(a[0]=='+'||a[0]=='-'||a[0]=='*'||a[0]=='*'||a[0]=='/'||a[0]=='%'){
        printf("----------------------->It is an  Arithmetic Operator<-----------------------\n");
    }
    else if(a[0]=='&'&&a[1]=='&'||a[0]=='|'&&a[1]=='|'||a[0]=='!') {
        printf("------------------------>It is  an logical  Operator<-------------------\n");
    }
    else if(a[0]=='&'||a[0]=='|'||a[0]=='^'||a[0]=='~'||a[0]=='<<'||a[0]=='>>'){
        printf("------------------------>It is  an Bitwise  Operator<-------------------\n");

    }
    else if(a[0]=='='&&a[1]==' '){
        printf("------------------------>It is  an Assignment  Operator<-------------------\n");
    }
    else if(a[0]=='='&&a[1]=='='||a[0]=='!'&&a[1]=='='||a[0]=='>'||a[0]=='<'||a[0]=='>'&&a[1]=='='||a[0]=='<'&&a[1]=='='){
             printf("------------------------>It is  an Relational   Operator<-------------------\n");

    }
    else{
        printf("--------------------------->It is not an Opeartor<------------------------------\n");   
    }


return 0;
}
